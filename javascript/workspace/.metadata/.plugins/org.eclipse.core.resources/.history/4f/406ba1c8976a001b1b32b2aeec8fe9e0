<!--
	pr("%", 5);
	결과 : %%%%%
	
	위의 결과가 나오는 함수를 선언하여 사용한다.
	단, 최소 한 번은 출력되어야 한다.
-->
</body>
<script>
	var msg = "최소 한 번은 출력되야 합니다."
	
	function pr(st, cnt){
		var result = st;
		if(cnt<1){
			alert(msg);
			return;
		}
		for(let i=1;i<=cnt;i++){
			document.write("<h1>st</h1>");
		}
	}
	pr("%",10);
</script>
